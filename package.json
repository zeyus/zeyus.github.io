		"touch": "^3.1.1",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.8.1",
		"vite": "^5.4.8",
		"vitest": "^2.1.2"
	},
	"type": "module",
	"dependencies": {
		"@eslint/object-schema": "^2.1.4",
		"@floating-ui/dom": "^1.6.11",
		"ansi_up": "^6.0.2",
		"flowbite": "^2.5.2",
